<!doctype html>
<html lang='en'>

	<head>
    	<title>Spark Dashboard</title>
    </head>
      
	
	<body>
	<% if flash[:notice]%>
		<div class="notice">
			<%= flash[:notice] %>
		</div>
	<%end%>

	<% if flash[:errors]%>
		<div class="errors">
			<%= flash[:errors] %>
		</div>
	<%end%>


	<%= yield %>

	</body>


</html>